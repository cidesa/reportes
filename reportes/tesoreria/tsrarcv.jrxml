FROM "SIMA005".OPORDPAG A 
INNER JOIN $P!{p_schema}.OPBENEFI B ON A.CEDRIF=B.CEDRIF 
LEFT  JOIN $P!{p_schema}.OPFACTUR C ON C.NUMORD=A.NUMORD 
INNER  JOIN $P!{p_schema}.OPRETORD D ON D.NUMORD=A.NUMORD
INNER JOIN $P!{p_schema}.tsrepret E ON D.CODTIP=E.CODRET
