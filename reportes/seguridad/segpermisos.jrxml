<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="segpermisos" pageWidth="612" pageHeight="792" columnWidth="572" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="p_schema" class="java.lang.String"/>
	<parameter name="p_titulo" class="java.lang.String"/>
	<parameter name="p_dirbase" class="java.lang.String"/>
	<parameter name="codusudes" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="codusuhas" class="java.lang.String"/>
	<parameter name="codapldes" class="java.lang.String"/>
	<parameter name="codaplhas" class="java.lang.String"/>
	<parameter name="codemp" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.loguse,d.nomuse,d.cedemp,c.nomemp,a.codapl,b.nomapl,a.nomopc,a.priuse,now() as hoy, a.desopc
from "SIMA_USER".apli_user a,"SIMA_USER".aplicacion b,"SIMA_USER".empresa c,"SIMA_USER".usuarios d
where a.loguse>=$P{codusudes}
and a.loguse<=$P{codusuhas}
and a.codapl>=$P{codapldes}
and a.codapl<=$P{codaplhas}
and a.codemp=$P{codemp}
and a.nomopc not in ('menu', 'admin', 'catalogo')
and a.codapl=b.codapl
and a.codemp=c.codemp
and a.loguse=d.loguse
order by a.loguse,a.codapl]]>
	</queryString>
	<field name="loguse" class="java.lang.String"/>
	<field name="nomuse" class="java.lang.String"/>
	<field name="cedemp" class="java.lang.String"/>
	<field name="nomemp" class="java.lang.String"/>
	<field name="codapl" class="java.lang.String"/>
	<field name="nomapl" class="java.lang.String"/>
	<field name="nomopc" class="java.lang.String"/>
	<field name="priuse" class="java.lang.String"/>
	<field name="hoy" class="java.sql.Timestamp"/>
	<field name="desopc" class="java.lang.String"/>
