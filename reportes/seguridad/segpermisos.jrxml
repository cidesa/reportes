					<textElement verticalAlignment="Middle">
						<font size="8"/>
					</textElement>
					<textFieldExpression class="java.lang.String"><![CDATA[$F{nomapl}]]></textFieldExpression>
				</textField>
				<staticText>
					<reportElement x="0" y="7" width="50" height="15"/>
					<box leftPadding="2"/>
					<textElement verticalAlignment="Middle">
						<font size="8" isBold="true"/>
					</textElement>
					<text><![CDATA[Módulo:]]></text>
				</staticText>
				<staticText>
					<reportElement x="0" y="22" width="377" height="15"/>
					<box leftPadding="2"/>
					<textElement verticalAlignment="Middle">
						<font size="8" isBold="true"/>
					</textElement>
					<text><![CDATA[Formulario]]></text>
				</staticText>
				<staticText>
					<reportElement x="377" y="22" width="128" height="15"/>
					<box leftPadding="2"/>
					<textElement textAlignment="Center" verticalAlignment="Middle">
						<font size="8" isBold="true"/>
					</textElement>
					<text><![CDATA[Acceso]]></text>
				</staticText>
			</band>
		</groupHeader>
	</group>
	<background>
		<band splitType="Stretch"/>
	</background>
	<pageHeader>
		<band height="105" splitType="Stretch">
			<image>
				<reportElement x="0" y="0" width="301" height="75"/>
				<imageExpression class="java.lang.String"><![CDATA[$P{p_dirbase}+"/img/logo_1.jpg"]]></imageExpression>
			</image>
			<textField>
				<reportElement x="0" y="75" width="572" height="26"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font size="14" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{p_titulo}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="429" y="22" width="103" height="20"/>
				<textElement textAlignment="Right"/>
				<textFieldExpression class="java.lang.String"><![CDATA["Página "+$V{PAGE_NUMBER}+" de"]]></textFieldExpression>
			</textField>
			<textField evaluationTime="Report">
				<reportElement x="532" y="22" width="40" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[" " + $V{PAGE_NUMBER}]]></textFieldExpression>
			</textField>
			<line>
				<reportElement x="0" y="101" width="572" height="1"/>
			</line>
		</band>
	</pageHeader>
	<columnHeader>
		<band height="49" splitType="Stretch">
			<rectangle>
				<reportElement x="0" y="0" width="572" height="49"/>
