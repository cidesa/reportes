	<detail>
		<band height="15" splitType="Stretch">
			<rectangle>
				<reportElement x="0" y="0" width="572" height="15"/>
			</rectangle>
			<textField>
				<reportElement x="377" y="0" width="128" height="15"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[( $F{priuse}.equals( new String("15") ) ? "Salvar-Eliminar-Consultar" : ( $F{priuse}.equals( new String("11") ) ? "Salvar-Consultar" : "Consultar" ) )]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="10" y="0" width="367" height="15"/>
				<textElement textAlignment="Left" verticalAlignment="Middle"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{desopc}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
