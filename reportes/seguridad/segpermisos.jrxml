				<textElement verticalAlignment="Middle">
					<font size="8" isBold="true"/>
				</textElement>
				<text><![CDATA[Nombre:]]></text>
			</staticText>
			<textField>
				<reportElement x="50" y="0" width="180" height="15"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{loguse}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="50" y="15" width="180" height="15"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{cedemp}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="50" y="30" width="522" height="15"/>
				<textElement verticalAlignment="Middle">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{nomuse}]]></textFieldExpression>
