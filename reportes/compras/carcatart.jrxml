	<queryString>
		<![CDATA[SELECT
A.CODART as articulo,
A.DESART as des,
A.CODCTA as codcta,
A.CODPAR as par,
A.EXITOT as exi,
A.UNIMED as medida,
b.nomram
FROM $P!{p_schema}.CAREGART A join $P!{p_schema}.caramart b on a.ramart=b.ramart
WHERE
A.CODART >= $P{art1} AND
A.CODART <= $P{art2} AND
A.ramart >= $P{ramartdes} AND
A.ramart <= $P{ramarthas} and
a.codpar like $P{comodin}
GROUP BY A.CODART,A.DESART,A.CODCTA,A.CODPAR,A.EXITOT,A.UNIMED, b.nomram
order by a.codart]]>
	</queryString>
