	<queryString>
		<![CDATA[SELECT a.reqart,
a.fecreq,
to_char(a.fecreq,'dd/mm/yyyy') as fechareq,
upper(a.desreq) as desreq,
'' as renglon,
b.canreq,
b.unimed,
b.desart,
a.obsreq,
substr(b.codcat,1,8) as codcat,
coalesce((select nompre from $P!{p_schema}.cpdeftit where codpre=substr(b.codcat,1,1)),'') as titulo,
coalesce((select nompre from $P!{p_schema}.cpdeftit where codpre=substr(b.codcat,1,8)),'') as proyecto
from $P!{p_schema}.casolart a,$P!{p_schema}.caartsol b
where a.reqart=b.reqart and a.reqart=$P{codreq}]]>
	</queryString>
